<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Eric Zimmerman</Authors>
    <Company>Eric Zimmerman</Company>
    <Product>Lnk parser</Product>
    <Description>Parses Windows shortcut files</Description>
    <Copyright>Eric Zimmerman</Copyright>
    <PackageProjectUrl>https://github.com/EricZimmerman/lnk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/EricZimmerman/lnk</RepositoryUrl>
    <AssemblyVersion>1.1.0.2</AssemblyVersion>
    <FileVersion>1.1.0.2</FileVersion>
    <Version>1.1.0.2</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ExtensionBlocks">
      <Version>1.1.0</Version>
    </PackageReference>
    <PackageReference Include="GuidMapping">
      <Version>1.0.0.1</Version>
    </PackageReference>
  </ItemGroup>
</Project>